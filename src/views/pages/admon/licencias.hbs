{{>tables}}

<div id="tbar">
    <button class="btn btn-secondary shadow-sm" onclick="$('#mainadmon').load('/admon/addlicencia').fadeIn('500');"><i
            class="fas fa-plus fa-fw me-2"></i>Agregar Licencia</button>
</div>

<table data-toggle="table" data-toolbar="#tbar" data-show-columns="false" data-pagination="true"
            data-search="true" data-show-jump-to="true" data-page-size='10' data-show-export="true" data-export-types="['csv','']" data-mobile-responsive="true" data-check-on-init="true"
            data-click-to-select="true" data-toolbar="#toolbar" class="table table-striped table-bordered table-sm"
            data-page-list="[50, 100, 200, All]" width="100%" id="dtable"
            class="table table-striped table-bordered table-responsive mt-3 shadow-sm">
    <thead class="table-light">
        <tr>
            <th data-field="nombres" data-sortable="true">Licencia</th>
            <th data-field="apellidos" data-sortable="true">Descripci√≥n</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each lics}}
        <tr>
            <td>{{licencia}}</td>
            <td>{{description}}</td>
            <td>
                <div class="btn-group btn-secondary shadow-sm w-100">
                    <button class="btn btn-sm btn-light" onclick="delDialog('{{_id}}', '{{licencia}}', 'remLicencia', 'Eliminar Licencia');" id="btn3" data-toggle="tooltip" data-placement="top" title="Eliminar Usuario"><i class="fas fa-trash fa-fw"></i></button>
                </div>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{>end}}
